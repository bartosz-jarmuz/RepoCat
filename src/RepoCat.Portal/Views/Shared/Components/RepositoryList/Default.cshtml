@model  RepositoriesListViewModel
@foreach (var name in @Model.Repositories)
{
    <li class="nav-item" style="padding-left: 15px;">
        <a class="nav-link" asp-area="" asp-controller="Repository" asp-action="Index" asp-route-repositoryName="@name" asp-route-view="base-breadcrumb">
            <i class="fa fa-box-open"></i> @name
        </a>
    </li>
}