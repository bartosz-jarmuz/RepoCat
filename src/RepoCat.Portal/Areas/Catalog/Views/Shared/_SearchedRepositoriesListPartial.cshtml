@using RepoCat.RepositoryManagement.Service;
@model IEnumerable<RepositoryQueryParameter>


    @foreach (RepositoryQueryParameter item in Model)
    {
        <a class="badge badge-primary tag badge-large" data-toggle="tooltip" data-placement="top" title="View repository content" data-delay='{"show":"500"}'
           href="@Url.Action("Index", "Repository", new { organizationName = item.OrganizationName, repositoryName = item.RepositoryName })">
            <i class="fab fa-codepen font-weight-light"></i>&nbsp;@item.OrganizationName - @item.RepositoryName
        </a>
    }


