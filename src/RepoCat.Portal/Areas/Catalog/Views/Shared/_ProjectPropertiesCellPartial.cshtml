@using RepoCat.Portal.Areas.Catalog.Models
@model ProjectInfoViewModel

@foreach (var property in Model.Properties)
{
    <partial name="_PropertyPartial" model="@(new PropertyViewModel(Model.Id, null, property))" />
}
@foreach (var component in Model.Components)
{
    foreach (var property in component.Properties)
    {
        <partial name="_PropertyPartial" model="@(new PropertyViewModel(Model.Id, component.Name, property))" />
    }
}