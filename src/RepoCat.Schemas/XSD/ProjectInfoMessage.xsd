<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ProjectInfoMessage" elementFormDefault="qualified"
    targetNamespace="todoo"
    xmlns="https://git.io/RepoCat ComponentManifest.xsd"
    xmlns:cmp="https://git.io/RepoCat-Component"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="https://git.io/RepoCat-Component.xsd"
             namespace="https://git.io/RepoCat-Component" />
  <xs:element name="Project">
    <xs:complexType>
      <xs:sequence>
        <xs:element type="xs:string" name="Name"/>
        <xs:element type="xs:string" name="AssemblyName"/>
        <xs:element type="xs:string" name="ProjectUri"/>
        <xs:element type="xs:string" name="RepositoryName"/>
        <xs:element type="xs:string" name="RepositoryStamp"/>
        <xs:element name="ComponentManifest" type="cmp:ComponentManifest" />
      </xs:sequence>
    </xs:complexType>
 </xs:element>
</xs:schema>
